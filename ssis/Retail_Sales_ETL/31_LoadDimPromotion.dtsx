                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH src AS (
  SELECT
    PromotionID,
    LTRIM(RTRIM(PromotionName)) AS PromotionName,
    LTRIM(RTRIM(StartDate))      AS StartDateTxt,
    LTRIM(RTRIM(EndDate))        AS EndDateTxt,
    LTRIM(RTRIM(DiscountPct))    AS DiscountTxt
  FROM stg.Promotions_raw
),
norm AS (
  SELECT
    PromotionID,
    PromotionName,
    TRY_CONVERT(date, NULLIF(StartDateTxt,'')) AS StartDate,
    TRY_CONVERT(date, NULLIF(EndDateTxt,''))   AS EndDate,
    TRY_CONVERT(decimal(5,2), REPLACE(DiscountTxt, '%','')) AS DiscountPct
  FROM src
),
ranked AS (
  SELECT *,
         ROW_NUMBER() OVER (
           PARTITION BY PromotionID
           ORDER BY ISNULL(EndDate,'9999-12-31') DESC,
                    ISNULL(StartDate,'1900-01-01') DESC,
                    ISNULL(DiscountPct,0) DESC
         ) AS rn
  FROM norm
)
SELECT PromotionID, PromotionName, StartDate, EndDate, DiscountPct
FROM ranked
WHERE rn = 1;</property>
